{
   "name": "app",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "next dev -p 3002",
      "build": "next build",
      "start": "next start -p 3002",
      "lint:check": "eslint \"src/**/*.+(js|jsx|ts|tsx|css)\"",
      "lint:error": "eslint --quiet \"src/**/*.+(js|jsx|ts|tsx|css)\"",
      "lint:fix": "eslint --fix",
      "format:check": "prettier --check \"src/**/*.+(js|jsx|ts|tsx|css|md)\"",
      "format:fix": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|css|md)\"",
      "lint-staged": "lint-staged",
      "commit": "git-cz",
      "prepare": "husky"
   },
   "config": {
      "commitizen": {
         "path": "@commitlint/cz-commitlint"
      }
   },
   "lint-staged": {
      "*.ts": [
         "npm run lint:check",
         "npm run format:fix",
         "git add ."
      ],
      "*.tsx": [
         "npm run lint:check",
         "npm run format:fix",
         "git add ."
      ]
   },
   "dependencies": {
      "@clerk/nextjs": "^4.29.5",
      "@hookform/resolvers": "^3.9.0",
      "@reduxjs/toolkit": "^1.9.3",
      "@tippyjs/react": "^4.2.6",
      "@types/node": "18.15.3",
      "@types/react": "18.0.28",
      "@types/react-dom": "18.0.11",
      "axios": "^1.3.5",
      "clsx": "^2.1.1",
      "cookies-next": "^4.1.0",
      "eslint-config-next": "13.2.4",
      "highlight.js": "^11.9.0",
      "http-proxy": "^1.18.1",
      "jsonwebtoken": "^9.0.2",
      "lightgallery": "^2.7.1",
      "next": "^13.4.2",
      "qs": "^6.11.2",
      "rc-pagination": "^3.3.1",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-hook-form": "^7.53.0",
      "react-icons": "^4.8.0",
      "react-quill": "^2.0.0",
      "react-redux": "^8.0.5",
      "react-toastify": "^9.1.3",
      "redux-thunk": "^2.4.2",
      "swiper": "^9.1.1",
      "swr": "^2.2.4",
      "typescript": "4.9.5",
      "uuid": "^9.0.1",
      "yup": "^1.4.0"
   },
   "devDependencies": {
      "@commitlint/cli": "^19.5.0",
      "@commitlint/config-conventional": "^19.5.0",
      "@commitlint/cz-commitlint": "^19.5.0",
      "@commitlint/prompt-cli": "^19.5.0",
      "@types/http-proxy": "^1.17.14",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/qs": "^6.9.11",
      "@types/quill": "^2.0.14",
      "@types/uuid": "^9.0.8",
      "autoprefixer": "^10.4.14",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "husky": "^9.1.6",
      "lint-staged": "^15.2.10",
      "postcss": "^8.4.21",
      "prettier": "^3.2.5",
      "sass": "^1.59.2",
      "tailwindcss": "^3.4.1"
   }
}
